# LangSmith Observability
# Set to 'true' to enable tracing of your agent's decision-making process
LANGCHAIN_TRACING_V2=true
# The endpoint for the LangSmith API
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
# Your personal LangSmith API key
LANGCHAIN_API_KEY=your_langsmith_api_key_here
# The name of the project in LangSmith where traces will be stored
LANGCHAIN_PROJECT=automaton-auditor

# LLM Providers
# Required for OpenAI-based agents or embeddings
OPENAI_API_KEY=your_openai_key_here
# Required if using Google Gemini-based models
GOOGLE_API_KEY=your_google_api_key_here
